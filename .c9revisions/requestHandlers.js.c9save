{"contributors":[],"silentsave":false,"ts":1349290415689,"patch":[[{"diffs":[[1,"\"use strict\";\r\n\r\nvar wrappers = require('./wrappers'),\r\n    fs = require('fs'),\r\n    util = require('util'); \r\n\r\n    \r\nfunction main(response) {\r\n    var meta = {\r\n        title: 'Draw on the Web!'\r\n    };\r\n    meta.css = ['styles/MainPage.css'];\r\n    meta.jscripts = ['/scripts/mainPage.js'];\r\n\r\n    fs.readFile(\"./Pages/MainForm.html\", function(err, content) {\r\n        if (err) {\r\n            throw err;\r\n        }\r\n        else {\r\n            var body = content;\r\n            wrappers.standard(meta, body, response);\r\n        }\r\n    });\r\n    \r\n}\r\n\r\n\r\nfunction staticFile(path,contentType, response)\r\n{\r\n     fs.readFile(path, function (err, content) {\r\n        if (err) {\r\n            throw err;\r\n        }\r\n        else{\r\n            response.setHeader('Content-Type', contentType);\r\n            response.writeHead(200);\r\n            response.write(content);\r\n            response.end();\r\n            util.log(\"Served\" + path);\r\n        }\r\n    });\r\n}\r\n\r\nexports.staticFile = staticFile;\r\nexports.main = main;"]],"start1":0,"start2":0,"length1":0,"length2":1013}]],"length":1013,"saved":false}
